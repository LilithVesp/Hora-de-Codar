programa
{//Escreva um algoritmo para imprimir os 10 primeiros números inteiros maiores que 100.
	
	funcao inicio()
	{inteiro numero=100, contador=0

	 enquanto(contador<10){
	 numero++
	 contador++
	 escreva(numero+"\n")
	 }
	
	}
}
